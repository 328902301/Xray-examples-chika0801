{
    "log": {
        "loglevel": "warning"
    },
    "dns": {
        "hosts": {
            "geosite:category-ads-all": "127.0.0.1",
            "domain:.mcdn.bilivideo.cn": "127.0.0.1",
            "domain:.szbdyd.com": "127.0.0.1",
            "regexp:cn-[a-zA-Z0-9-]+\\.bilivideo\\.com": "127.0.0.1",
            "regexp:[a-zA-Z0-9-]+-pcdn-[a-zA-Z0-9-]+\\.biliapi\\.net": "127.0.0.1"
        },
        "servers": [
            "https+local://223.5.5.5/dns-query",
            {
                "address": "fakedns",
                "domains": [
                    "geosite:geolocation-!cn"
                ],
                "skipFallback": true,
                "queryStrategy": "UseIPv4"
            }
        ],
        "queryStrategy": "UseIP"
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "inboundTag": [
                    "dns-in"
                ],
                "outboundTag": "dns-out"
            },
            {
                "port": 853,
                "network": "tcp",
                "outboundTag": "block"
            },
            {
                "port": 443,
                "network": "udp",
                "outboundTag": "block"
            },
            {
                "domain": [
                    "geosite:category-ads-all",
                    "domain:.mcdn.bilivideo.cn",
                    "domain:.szbdyd.com",
                    "regexp:cn-[a-zA-Z0-9-]+\\.bilivideo\\.com",
                    "regexp:[a-zA-Z0-9-]+-pcdn-[a-zA-Z0-9-]+\\.biliapi\\.net"
                ],
                "outboundTag": "block"
            },
            {
                "domain": [
                    "geosite:geolocation-!cn"
                ],
                "outboundTag": "proxy"
            },
            {
                "ip": [
                    "geoip:cn",
                    "geoip:private"
                ],
                "outboundTag": "direct"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 10808,
            "protocol": "socks",
            "settings": {
                "udp": true
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic",
                    "fakedns"
                ]
            }
        },
        {
            "listen": "127.0.0.1",
            "port": 10853,
            "protocol": "dokodemo-door",
            "settings": {
                "address": "223.5.5.5",
                "port": 53,
                "network": "udp"
            },
            "tag": "dns-in"
        }
    ],
    "outbounds": [
        {
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "",
                        "port": 443,
                        "users": [
                            {
                                "id": "chika",
                                "encryption": "none",
                                "flow": "xtls-rprx-vision"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "",
                    "fingerprint": "chrome"
                },
                "sockopt": {
                    "tcpMptcp": true,
                    "tcpNoDelay": true
                }
            },
            "mux": {
                "enabled": true,
                "concurrency": -1,
                "xudpConcurrency": 16,
                "xudpProxyUDP443": "reject"
            },
            "tag": "proxy"
        },
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "ForceIPv6v4"
            },
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "tag": "block"
        },
        {
            "protocol": "dns",
            "settings": {
                "nonIPQuery": "drop"
            },
            "proxySettings": {
                "tag": "direct"
            },
            "tag": "dns-out"
        }
    ]
}
